{
  "name": "textract",
  "version": "1.1.2",
  "homepage": "https://github.com/dbashford/textract",
  "author": "David Bashford",
  "description": "Extracting text from files of various type including html, pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf, text/*, and various open office.",
  "contributors": [
    {
      "name": "David Bashford",
      "email": "dbashford@hotmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dbashford/textract"
  },
  "keywords": [
    "textract",
    "extract",
    "html",
    "csv",
    "text",
    "pdf",
    "docx",
    "doc",
    "xls",
    "xlsx",
    "png",
    "jpg",
    "gif",
    "rtf",
    "dxf",
    "pptx",
    "html",
    "markdown",
    "xml",
    "odt",
    "ott",
    "xlsb",
    "xlsm",
    "xltx",
    "ods",
    "ots",
    "potx",
    "odg",
    "otg"
  ],
  "dependencies": {
    "mime" : "1.3.4",
    "pdf-text-extract" : "1.1.4",
    "xpath" : "0.0.9",
    "xmldom" : "0.1.19",
    "j": "0.4.3",
    "cheerio":"0.19.0",
    "marked": "0.3.3",
    "meow": "2.0.0",
    "got": "3.3.1",
    "html-entities": "1.1.3",
    "yauzl": "2.3.1"
  },
  "devDependencies": {
    "mocha": "1.9.0",
    "chai": "1.5.0"
  },
  "config": {
    "image": "sidhuko/textract:0.0.1"
  },
  "scripts": {
     "test": "node_modules/.bin/mocha",
     "docker:test": "npm run docker -- sh -c 'npm run test'",
     "docker": "docker run --name $npm_package_name --rm --env NODE_ENV=development -v $(pwd):/worker -w /worker ${npm_package_config_image}",
     "docker:ssh": "docker run --rm --env NODE_ENV=development -v $(pwd):/worker -w /worker -t -i ${npm_package_config_image} /bin/bash"
   },
  "license": "MIT",
  "engines": {
    "node": ">=0.8"
  },
  "bin": {
    "textract": "./bin/textract"
  },
  "main": "./lib/index"
}
